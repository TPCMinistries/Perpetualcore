{
  "functions": {
    "app/api/documents/upload/route.ts": {
      "maxDuration": 120
    },
    "app/api/documents/process/route.ts": {
      "maxDuration": 120
    },
    "app/api/documents/[id]/summary/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/proactive-nudges",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/process-sequences",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/process-agents",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/usage-alerts",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/trial-reminders",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/billing-sync",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/process-webhooks",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/sync-usage",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/heartbeat",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/sweep-plans",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/refresh-tokens",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/cron/finalize-overages",
      "schedule": "0 23 * * *"
    }
  ]
}
